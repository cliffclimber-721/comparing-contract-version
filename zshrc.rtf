export PATH="/opt/homebrew/bin:$PATH"

export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \\. "$NVM_DIR/nvm.sh"  # This loads nvm
[ -s "$NVM_DIR/bash_completion" ] && \\. "$NVM_DIR/bash_completion"  # This loads nvm bash_completion

alias gethRPCWS='geth --networkid 4532 --http --http.port 8545 --http.corsdomain "*" --http.addr "0.0.0.0" --http.api "admin,eth,debug,miner,netxpool,personal,web3" --ws --ws.port 8546 --ws.origins "*" --ws.api "admin,eth,debug,miner,net,txpool,personal,web3" console --allow-insecure-unlock --mine --miner.threads 1 --unlock 0 --password ./password'}

#networkid has to be designated by your own number.
